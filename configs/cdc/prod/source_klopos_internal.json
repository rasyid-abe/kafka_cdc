	"key.converter": "io.confluent.connect.avro.AvroConverter",
	"database.user": "${file:/home/appuser/prod-connector-klopos_internal.properties:user}",
	"database.history.kafka.bootstrap.servers": "kafka1:19091,kafka2:29091,kafka3:39091",
	"time.precision.mode": "connect",
	"database.server.name": "debezium.db_klopos_internal",
	"database.port": "3306",
	"key.converter.schemas.enable": "false",
	"value.converter.schema.registry.url": "http://schema-registry:8081",
	"database.serverTimezone": "Asia/Jakarta",
	"database.hostname": "${file:/home/appuser/prod-connector-klopos_internal.properties:hostname}",
	"database.password": "${file:/home/appuser/prod-connector-klopos_internal.properties:password}",
	"value.converter.schemas.enable": "false",
	"name": "debezium.db_klopos_internal 20230704",
	"table.include.list": "klopos_internal.M_User_recap",
	"key.converter.schema.registry.url": "http://schema-registry:8081",
	"database.include.list": "klopos_internal"
